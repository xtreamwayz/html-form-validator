--TEST--
Test data validators attribute
--HTML-FORM--
<form action="/" method="post">
    <div class="input-group">
        <input type="text" id="name" name="name"
               data-validators="stringlength{min:32,max:140}|alpha{allowWhiteSpace:0}|notempty"
        />
    </div>
</form>
--DEFAULT-VALUES--
{
    "name": "Bruce Wayne"
}
--SUBMITTED-VALUES--
{
    "name": "  Batman  "
}
--EXPECTED-VALUES--
{
    "name": "  Batman  "
}
--EXPECTED-FORM--
<form action="/" method="post">
    <div class="input-group has-danger">
        <input type="text" id="name" name="name" value="Bruce Wayne" />
        <div class="text-danger">The input contains non alphabetic characters</div>
        <div class="text-danger">The input is less than 32 characters long</div>
    </div>
</form>
--EXPECTED-ERRORS--
{
    "name": [
        "notAlpha",
        "stringLengthTooShort"
    ]
}
